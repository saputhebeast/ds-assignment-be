    "clean-dir": "rimraf dist",
    "copy": "babel src --out-dir ./dist --copy-files",
    "dev": "nodemon --exec babel-node src/server.js",
    "start": "node dist/server.js",
    "format:code": "prettier --write \"src/**/*.js\"",
    "format:test": "prettier --write \"test/**/*.js\"",
    "format": "npm run format:code && npm run format:test",
    "lint": "eslint \"src/**/*.js\""
  },
